{
  "name": "solana-stack-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/solana-stack-backend/main",
    "start:dev:all": "concurrently --colors \"auto\" -n \"buy,launch,swap,run,user,mint\" \"nest start buy --watch\" \"nest start launch --watch\" \"nest start swap --watch\" \"nest start run --watch\" \"nest start user --watch\" \"nest start mint --watch\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/solana-stack-backend/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-crypto/client-node": "^4.0.0",
    "@aws-sdk/client-kms": "^3.574.0",
    "@aws-sdk/client-s3": "^3.583.0",
    "@azure/web-pubsub": "^1.1.1",
    "@azure/web-pubsub-client": "^1.0.1",
    "@fastify/multipart": "^9.0.1",
    "@metaplex-foundation/mpl-token-metadata": "^2.13.0",
    "@metaplex-foundation/umi": "^0.9.1",
    "@metaplex-foundation/umi-bundle-defaults": "^0.9.1",
    "@metaplex-foundation/umi-bundle-tests": "^0.9.1",
    "@metaplex-foundation/umi-serializers": "^0.9.0",
    "@metaplex-foundation/umi-uploader-nft-storage": "^0.9.1",
    "@metaplex-foundation/umi-web3js-adapters": "^0.9.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.4.15",
    "@nestjs/terminus": "^10.2.3",
    "@pinata/sdk": "^2.1.0",
